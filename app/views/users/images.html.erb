 <div class="quietview">
    <div class="name-plate">
        <h1><%= @user.name %> さん</h1>
    </div>

    <%= link_to 'アカウント設定', edit_user_path(current_user) %>

    <div class="viewtest">
    
        <ul class="setting-list">
            <li><%= link_to '仕分けruの設定', user_path(current_user) %></li>
            <li><%= link_to '疑似ディレクトリ予定', "/users/#{params[:id]}/images" %></li>
            <li><%= link_to 'アカウント設定', edit_user_path(current_user) %></li>
        </ul>

        <div class="setting">
            <div class="user_image-list">
                <% if @user %>
                    <% @user.images.each do |image|%>
                        <%= image_tag image.url %>
                    <% end %>
                <% end %>
            </div>
        </div>
    </div>
</div>